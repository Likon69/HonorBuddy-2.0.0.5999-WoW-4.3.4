					{
						if (IsQuestCompleted(12720))
							return false;

						_submittedScarlet = ObjectManager.GetObjectsOfType<WoWUnit>().FirstOrDefault(x => (x.Entry == 28610 || x.Entry == 28936 || x.Entry == 28939 || x.Entry == 28940) && x.IsAlive && !x.Attackable && x.DistanceSqr <= 20f*20f);


						if (StyxWoW.Me.Inventory.Equipped.MainHand == null || !(StyxWoW.Me.Inventory.Equipped.MainHand.Entry == 142274 || StyxWoW.Me.Inventory.Equipped.MainHand.Entry == 39371))

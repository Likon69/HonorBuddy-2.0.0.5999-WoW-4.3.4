						<While Condition="Me.ZoneId != 7078" >
							<Code File="Misc\RunLua" Lua="UseItemByName(110560);" WaitTime="500" />
							<Code File="WaitTimer" WaitTime="50000" TerminateWhen="Me.ZoneId == 7078 || CastingInterruptedOutland()" />
